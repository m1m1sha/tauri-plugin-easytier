{
  "compilerOptions": {
    "target": "ES2023",
    "baseUrl": ".",
    "rootDir": "./",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "paths": {
      "types": [
        "@types"
      ]
    },
    "allowJs": true,
    "strict": true,
    "declaration": true,
    "declarationDir": "../webview-dist",
    "outDir": "../webview-dist",
    "esModuleInterop": true
  },
  "exclude": [
    "../webview-dist",
    "../node_modules"
  ]
}
